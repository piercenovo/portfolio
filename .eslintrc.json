{
  "env": {
    "node": true
  },
  "extends": [
    "./node_modules/standard/eslintrc.json",
    "next/core-web-vitals"
  ],
  "rules": {
    "space-before-function-paren": "off",
    "no-undef": "off",
    "array-callback-return": "off",
    "react-hooks/exhaustive-deps": "off",
    "@next/next/no-img-element": "off",
    "quotes": [2, "single"]
  }
}
